
local hitbox = game:GetService("ReplicatedStorage").Assets.Hitboxes

while true do
hitbox.BicycleKick.Size = Vector3.new(50, 50, 50)
hitbox.Tackle.Size = Vector3.new(50,50,50)
hitbox.Dive.LeftDive.Size = Vector3.new(50,50,50)
hitbox.Dive.RightDive.Size = Vector3.new(50,50,50)
hitbox.Dive.FrontDive.Size = Vector3.new(50,50,50)
wait(0)
end