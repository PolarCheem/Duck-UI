local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("JJS", "Default")

local tab = DrRayLibrary.newTab("Main", "ImageIdHere")
local tab2 = DrRayLibrary.newTab("Settings", "ImageIdHere")

tab.newInput("Main Colors", "Should format: 10, 10, 10", function(text)
local mainColor = Color3.fromRGB(text) -- Customize as you wish; these are in RGB format. (mainColor applies to main colors like background, buttons, etc.)

window:SetTheme(mainColor)
end)

tab.newInput("Secondary Colors", "should format 10, 10, 10", function(text)
local secondColor = Color3.fromRGB(text) -- Secondary Color; applies to Toggle when activated and slider background.

window:SetTheme(secondColor)
end)

tab2.newInput("Custom Ultimate Name", "Renames your ultimate", function(rename)
local ult = game.Players.LocalPlayer.PlayerGui.Main.Ultimate.Title

ult.Text = rename
end)

tab2.newInput("First Move Rename", "Prints your input.", function(rename)
local movesetFrame = game.Players.LocalPlayer.PlayerGui.Main.Moveset

local children = movesetFrame:GetChildren()

for _, child in pairs(children) do
if child:IsA("Frame") and (child.Name == "Lapse Blue" or child.Name == "Cursed Strikes" or child.Name == "Reserve Balls" or child.Name == "Rabbit Escape" or child.Name == "Stockpile") then
local itemFrame = child:FindFirstChild("ItemName")
if itemFrame then
itemFrame.Text = rename
end
end
end
end)

tab2.newInput("Second Move Rename", "Prints your input.", function(rename)
local movesetFrame = game.Players.LocalPlayer.PlayerGui.Main.Moveset

local children = movesetFrame:GetChildren()

for _, child in pairs(children) do
if child:IsA("Frame") and (child.Name == "Reversal Red" or child.Name == "Crushing Blow" or child.Name == "Shutter Doors" or child.Name == "Nue" or child.Name == "Soul Fire") then
local itemFrame = child:FindFirstChild("ItemName")
if itemFrame then
itemFrame.Text = rename
end
end
end
end)

tab2.newInput("Third Move Rename", "Prints your input.", function(rename)
local movesetFrame = game.Players.LocalPlayer.PlayerGui.Main.Moveset

local children = movesetFrame:GetChildren()

for _, child in pairs(children) do
if child:IsA("Frame") and (child.Name == "Rapid Punches" or child.Name == "Divergent Fist" or child.Name == "Rough Energy" or child.Name == "Toad" or child.Name == "Focus Strike") then
local itemFrame = child:FindFirstChild("ItemName")
if itemFrame then
itemFrame.Text = rename
end
end
end
end)

tab2.newInput("Fourth Move Rename", "Prints your input.", function(rename)
local movesetFrame = game.Players.LocalPlayer.PlayerGui.Main.Moveset

local children = movesetFrame:GetChildren()

for _, child in pairs(children) do
if child:IsA("Frame") and (child.Name == "Twinfold Kick" or child.Name == "Manji Kick" or child.Name == "Fever Break" or child.Name == "Body Repel" or child.Name == "Divine Dog: Totality") then
local itemFrame = child:FindFirstChild("ItemName")
if itemFrame then
itemFrame.Text = rename
end
end
end
end)

