task.wait(1)
local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/PolarCheem/Duck-UI/refs/heads/main/better%20ui"))()

local win = lib:Create("RobloxCoreGui", "FF2")

local Config = {
    MagsOn = false,
    MagDistance = 10,
    Pull_Vector = false,
    Football_Vector = 0
}

local function enableMags()
    local player = game:GetService("Players").LocalPlayer

    while Config.MagsOn do
        task.wait(0)
        for _, value in pairs(workspace:GetChildren()) do
            if value.Name == "Football" and value:IsA("BasePart") then
                local catchRight = player.Character:FindFirstChild("CatchRight")
                if catchRight and (catchRight.Position - value.Position).Magnitude <= Config.MagDistance then
                    firetouchinterest(catchRight, value, 0)
                    firetouchinterest(catchRight, value, 1)
                    task.wait()
                    firetouchinterest(catchRight, value, 0)
                    firetouchinterest(catchRight, value, 1)
                end
            end
        end
    end
end

task.spawn(function()
    game:GetService("RunService").Heartbeat:Connect(function()
        for _, value in pairs(workspace:GetChildren()) do
            if value.Name == "Football" and value:IsA("BasePart") then
                local player = game:GetService("Players").LocalPlayer
                local direction = (value.Position - player.Character.HumanoidRootPart.Position).Unit

                if player:DistanceFromCharacter(value.Position) <= Config.Football_Vector and Config.Pull_Vector then
                    player.Character.HumanoidRootPart.Velocity = direction * Config.Football_Vector
                end
            end
        end
    end)
end)


local magnetTab = win:tab("Catch", true)

magnetTab:toggle("Magnet Enabled", Config.MagnetEnabled, function(value)
    Config.MagnetEnabled = value
    if Config.MagnetEnabled then
        enableMagnet()
    end
end)

magnetTab:input("Magnet Distance", "", false, function(value)
    local numValue = tonumber(value) or 10
    Config.FootballDistanceCatch = math.clamp(numValue, 0, 20)
end)

magnetTab:toggle("Vector Toggle", false, function(value)
    Config.Pull_Vector = value
end)

magnetTab:input("Vector Distance", "", false, function(value)
    local numValue = tonumber(value) or 0
    Config.Football_Vector = math.clamp(numValue, 0, 100)
end)

local fullbright = false

MiscTab:toggle("Fullbright Toggle", false, function(value)
fullbright = v
while fullbright do
	Lighting.Brightness = 2
	Lighting.ClockTime = 14
	Lighting.FogEnd = 100000
	Lighting.GlobalShadows = false
	Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
wait(0)
end
end)

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function onCharacterAdded(character)
    applyJumpBoost(character)
end

if player.Character then
    onCharacterAdded(player.Character)
end

player.CharacterAdded:Connect(onCharacterAdded)