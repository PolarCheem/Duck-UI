local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("Virtual Football", "Default")

local tab = DrRayLibrary.newTab("Main", "ImageIdHere")
local tab2 = DrRayLibrary.newTab("Foot", "ImageIdHere")
local tab3 = DrRayLibrary.newTab("Spoof", "ImageIdHere")

local notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/wrestonbest/Helixia/main/Scripts/notification.lua"))()
    notification({
        Title = "Loading...",  
        Text = "Please wait a second!", 
        Duration = 2
    })

local notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/wrestonbest/Helixia/main/Scripts/notification.lua"))()
    notification({
        Title = "Loaded",  
        Text = "Thx for using", 
        Duration = 3
    })

local gui = Instance.new("ScreenGui")
gui.Name = "patrickGui"
gui.Parent = game.CoreGui

--///patrick gui maker///--


local main = Instance.new("Frame")
main.Size = UDim2.new(0.01, 0, 0.02, 0)
main.Position = UDim2.new(0.5, 0, 0.48, 0)
main.AnchorPoint = Vector2.new(0.5, 0.5) -- Center the frame
main.BackgroundColor3 = Color3.new(1, 1, 1)
main.BorderColor3 = Color3.new(0, 0, 0)
main.BorderSizePixel = 1
main.Active = true
main.BackgroundTransparency = 0
main.Draggable = false
main.Parent = gui
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = main

tab.newButton("Inf stamina ", "Prints Hello!", function()
game:GetService("Players").LocalPlayer.Data.Stamina.Value = 999999999999
end)

tab2.newSlider("Foot Reach", "Epic slider", 100, false, function(num)
game.Players.LocalPlayer.Character.HitBoxExpansion.Size = Vector3.new(num, num, num)
end)
tab2.newSlider("Block Reach", "Epic slider", 100, false, function(num)
game.Players.LocalPlayer.Character.Block.Size = Vector3.new(num, num, num)
end)

tab3.newSlider("Strength Stat", "Epic slider", 10, false, function(num)
game.Players.LocalPlayer.AttributeValues.Strength.Value = num
end)

tab3.newSlider("Technique Stat", "Epic slider", 10, false, function(num)
game.Players.LocalPlayer.AttributeValues.Technique.Value = num
end)

tab3.newSlider("Athleticism Stat", "Epic slider", 10, false, function(num)
game.Players.LocalPlayer.AttributeValues.Athleticism.Value = num
end)

tab3.newSlider("Dribble Stat", "Epic slider", 10, false, function(num)
game.Players.LocalPlayer.AttributeValues.Dribbling.Value = num
end)

tab3.newSlider("defence Stat", "Epic slider", 10, false, function(num)
game.Players.LocalPlayer.AttributeValues.Defence.Value = num
end)

tab3.newInput("Change Perk", "Put perk name", function(text)
game.Players.LocalPlayer.AttributeValues.Perk.Value = text
end)
